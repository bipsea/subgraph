{
  "name": "bipsea",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy:goerli": "graph deploy --product hosted-service leon-do/bipsea-goerli --network goerli",
    "deploy:matic": "graph deploy --product hosted-service leon-do/bipsea-matic --network matic",
    "deploy:optimism": "graph deploy --product hosted-service leon-do/bipsea-optimism --network optimism",
    "create-local": "graph create --node http://localhost:8020/ bipsea",
    "remove-local": "graph remove --node http://localhost:8020/ bipsea",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 bipsea"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.32.0",
    "@graphprotocol/graph-ts": "0.27.0"
  },
  "devDependencies": {
    "mustache": "^4.2.0"
  }
}
